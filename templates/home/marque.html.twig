   }
   .radio-item {
  display: inline-block;
  position: relative;
  padding: 0 6px;
  margin: 10px 0 0;
}

.radio-item input[type='radio'] {
  display: none;
}

.radio-item label {
  color: #666;
  font-weight: normal;
}

.radio-item label:before {
  content: " ";
  display: inline-block;
  position: relative;
  top: 5px;
  margin: 0 5px 0 0;
  width: 35px;
   height: 35px;
  border-radius: 100%;
  border:4px solid #258152;
  background-color: transparent;
  left: -5px;

   background: #fff;
}
.radio-item input[type=radio] + label:after {
   background: #fff;
}

.radio-item input[type=radio]:checked + label:after {
   border-radius: 100%;
   width: 35px;
   height: 35px;
   position: absolute;
   top: 5px;
   left: -5px;
   content: " ";
   display: block;
   background: #258152;
   background-image: url(https://i.imgur.com/CK1h7ap.png);
   background-size: 100% 100%;
}
  @media(max-width: 600px){
      .products-pack > div {
          width: 100%;
      }
   }
.radio-item label{
   position: absolute;
   top: -35px;
   right: -20px;
}
.custom-control.active {
    background-image: linear-gradient(90deg, rgb(181, 215, 176) 0%, rgb(198, 234, 193) 55%, rgb(255, 255, 255) 100%);
    box-shadow: 3px 4px 1px #f0f0f0;
    transition: all .7s ease;
}
</style>
